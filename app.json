{
  "expo": {
    "name": "share-extension-expo-example",
    "slug": "share-extension-expo-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "wtf.uku.client.local",
      "buildNumber": "1"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.0"
          }
        }
      ],
      [
        "share-extension-expo-plugin",
        {
          "devTeam": "X7H82CY5HL",
          "extensionPath": "assets/ios/shareExtension",
          "mainStoryboardName": "ShareView",
          "activationRule": ["text", "image", "url"]
        }
      ]
    ]
  }
}
